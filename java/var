public class Main {
    public static void main(String[] args) {
        System.out.print("Loading");
        try {
            Thread.sleep(1000); // 1초 대기
        } catch (InterruptedException e) {
            e.printStackTrace();
        }
        System.out.print("\r"); // 캐리지 리턴
        System.out.print("Completed");
    }
}
